{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "description": "react ts template",
  "author": {
    "name": "macus"
  },
  "license": "MIT",
  "homepage": "https://github.com/Xukaixin97/vite-react-ts-template",
  "scripts": {
